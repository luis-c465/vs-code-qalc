{
    "name": "vs-calc",
    "displayName": "VSCalc",
    "description": "Interactive scratchpad calculator.",
    "publisher": "nortakales",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.44.0"
    },
    "keywords": [
        "calc",
        "calculator",
        "math"
    ],
    "categories": [
        "Other"
    ],
    "homepage": "https://github.com/sagebind/mathpad",
    "repository": {
        "type": "git",
        "url": "https://github.com/sagebind/mathpad.git"
    },
    "bugs": {
        "url": "https://github.com/sagebind/mathpad/issues"
    },
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "languages": [
            {
            "id": "qalc",
            "extensions": [".qalc"]
            }
        ],
        "grammars": [
            {
            "language": "qalc",
            "scopeName": "source.qalc",
            "path": "./syntaxes/qalc.tmGrammar.json"
            }
        ]
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint"
    },
    "dependencies": {
        "@types/mathjs": "^6.0.5",
        "axios": "^0.21.1",
        "mathjs": "^6.6.4"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/vscode": "^1.44.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "vscode-test": "^1.3.0"
    }
}
